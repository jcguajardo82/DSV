@using ServicesManagement.Web.Models.ProcesoSurtido;

<div class="modal inmodal fade" id="dlgReasigAutOr" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header pt-3 pb-1">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Autorización de Reasignación de Orden</h4>
            </div>
            <div class="modal-body">
                <form id="frmAutorizaReasignacion">
                    <div class="row">
                        <div class="col col-xs-12 col-md-12">
                            <div class="form-group">
                                <label>Clave de Autorización:</label>
                                    <input type="password" id="txtPassAut" class="form-control" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="ReasigAutOrden()">Aceptar</button>
            </div>
        </div>
    </div>
</div>
