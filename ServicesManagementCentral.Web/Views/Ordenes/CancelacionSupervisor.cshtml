@functions{
    /**/

    public string ColoresSemaforo(string numColor)

    {

        string estilo = string.Empty;

        switch (numColor)

        {

            case "1":

                estilo = "rowVerde";

                break;

            case "2":

                estilo = "rowAmarillo";

                break;

            case "3":

                estilo = "rowRojo";

                break;



            default:

                estilo = "rowInfo";

                break;

        }

        return estilo;

    }

