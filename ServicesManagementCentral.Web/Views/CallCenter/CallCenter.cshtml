                                        <td>Nombre:</td>
                                        <td><b id="lblNombreCte"></b></td>
                                    </tr>
                                    <tr>
                                        <td>Folio del Cliente:</td>
                                        <td>
                                            <p id="lblFolioCte"></p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Correo Electr&oacute;nico:</td>
                                        <td>
                                            <p id="lblClaveCte"></p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>TarjetaLealtad:</td>
                                        <td>
                                            <p id="lblTarjetaLealtad"></p>
                                        </td>
                                    </tr>
